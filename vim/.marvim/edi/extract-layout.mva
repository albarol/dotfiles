BEGIN { column=1; }
NR == 1 { edi=$0; }
NR > 1 {
  if (match($0, /#/) == 0) {
    size = gensub(/\s*([a-zA-Z0-9_]+)\s?=\s?.*\(([0-9]+)\).*/, "\\2", "g");
    key = toupper(gensub(/\s*([a-zA-Z0-9_]+)\s?=\s?.*\(([0-9]+)\).*/, "\\1", "g"));
    printf "%s = \"%s\"\n", key, substr(edi, column, size)
    column = column + size
  }
}
